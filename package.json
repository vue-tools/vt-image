{
  "name": "vt-image",
  "version": "0.1.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-tools/vt-image"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-tools/vt-image/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-vue-jsx",
      "transform-runtime"
    ]
  },
  "homepage": "https://github.com/vue-tools/vt-image#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.3.2",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.10.1",
    "gh-pages": "^0.12.0",
    "istanbul": "^0.4.5",
    "karma": "^1.5.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-promise": "^0.1.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "postcss-px2rem": "^0.3.0",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "vue": "^2.1.10",
    "vue-docs": "^1.5.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^9.7.0",
    "vue-style-loader": "^2.0.1",
    "vue-template-compiler": "^2.1.10",
    "webp-webpack-plugin": "^1.2.2",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^2.4.1"
  },
  "scripts": {
    "test": "cross-env BABEL_ENV=test ./node_modules/karma/bin/karma start ./tests/karma.conf.js",
    "prepublish": "npm run compile",
    "codecov": "./node_modules/.bin/codecov",
    "copy": "rm -fr dist && cp -rf src dist",
    "docs": "./node_modules/.bin/vue-docs start",
    "docs:build": "rm -fr docs && ./node_modules/.bin/vue-docs build && node gh-pages.js $1",
    "compile": "npm run copy && node_modules/.bin/babel dist -d dist",
    "tag": "node .tag add $1",
    "tag:delete": "git tag -d $1 && git push origin --delete tag $1"
  }
}
